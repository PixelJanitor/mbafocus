  .calendar-container
    script#calendar type="text/template"
      | <div class="clndr-controls">
      |   <div class="clndr-previous-button">&lsaquo;</div>
      |   <div class="month"><%= month %></div>
      |   <div class="clndr-next-button">&rsaquo;</div>
      | </div>
      | <div class="clndr-grid primary-background">
      |   <div class="days-of-the-week">
      |     <% _.each(daysOfTheWeek, function(day) { %>
      |       <div class="header-day"><%= day %></div>
      |     <% }); %>
      |   <div class="days">
      |     <% _.each(days, function(day) { %>
      |       <div class="<%= day.classes %>"><%= day.day %></div>
      |     <% }); %>
      |   </div>
      |   </div>
      | </div>
      | <div class="event-listing">
      |   <div class="event-listing-title text-center">Upcoming Events</div>
      |   <% _.each(eventsThisMonth, function(event) { %>
      |      <div class="event-item">
      |       <div class="event-details">
      |         <span class="event-item-name"><%= event.name %></span>
      |         <span class="event-item-date"><%= event.date %></span>
      |       </div>
      |       <div class="event-item-location">At: <%= event.location %></div>
      |      </div>
      |   <% }); %>
      | </div>
